{
    "name": "chenjiabin/think-swoole",
    "description": "Swoole extend for thinkphp5.0",
    "license": "Apache-2.0",
    "type": "think-extend",
    "authors": [
        {
            "name": "橙加冰"
        }
    ],
    "require": {
	"ext-swoole": ">=1.9.5",
	"topthink/think-installer": "^2.0",
    "topthink/framework": "5.0.*",
    "jeremeamia/superclosure": "^2.4",
    "xavier/xcron-expression": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "think\\swoole\\": "src"
        },
        "files": [
            "src/command.php"
        ]
    },
    "extra": {
        "think-config": {
        "swoole": "src/config/swoole.php",
        "swoole_server":"src/config/server.php",
        "timer":"src/config/timer.php"
        }
    }
}